<?xml version="1.0" ?>
<URIHandlers>
	<URIHandler name="DefaultModuleActionPath" pattern="/%module%/%action%.html" method="GET" handler="YageCMS.Core.Tools.Module->CallModule">
		<Parameter name="module" pattern="([a-zA-Z0-9_]+)" />
		<Parameter name="action" pattern="([a-zA-Z0-9_]+)" />
	</URIHandler>
	
	<URIHandler name="DefaultModuleActionPathWithID" pattern="/%module%/%id%/%action%.html" method="GET" handler="YageCMS.Core.Tools.Module->CallModule">
		<Parameter name="module" pattern="([a-zA-Z0-9_]+)" />
		<Parameter name="id" pattern="([A-F0-9\-]{36})" />
		<Parameter name="action" pattern="([a-zA-Z0-9_]+)" />
	</URIHandler>
</URIHandlers>
